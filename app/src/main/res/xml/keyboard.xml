<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="100%p"
    android:keyHeight="46dp"
    android:horizontalGap="6dp"
    android:verticalGap="5dp">

    <!--1-5-->
    <Row>
        <Key
            android:codes="@string/keyboard_code_plus_10"
            android:keyEdgeFlags="left"
            android:keyLabel="+10" />

        <Key
            android:codes="49"
            android:keyLabel="1" />

        <Key
            android:codes="50"
            android:keyLabel="2" />

        <Key
            android:codes="51"
            android:keyEdgeFlags="right"
            android:keyLabel="3" />
    </Row>

    <!--6-0-->
    <Row>
        <Key
            android:codes="@string/keyboard_code_plus_50"
            android:keyEdgeFlags="left"
            android:keyLabel="+50" />

        <Key
            android:codes="52"
            android:keyLabel="4" />

        <Key
            android:codes="53"
            android:keyLabel="5" />

        <Key
            android:codes="54"
            android:keyEdgeFlags="right"
            android:keyLabel="6" />
    </Row>

    <Row>
        <Key
            android:codes="@string/keyboard_code_max"
            android:keyEdgeFlags="right"
            android:keyLabel="MAX" />

        <Key
            android:codes="55"
            android:keyLabel="7" />

        <Key
            android:codes="56"
            android:keyLabel="8" />

        <Key
            android:codes="57"
            android:keyEdgeFlags="right"
            android:keyLabel="9" />
    </Row>

    <Row>
        <Key
            android:codes="@string/keyboard_code_done"
            android:keyEdgeFlags="right"
            android:keyLabel="@string/keyboard_done" />

        <Key
            android:codes="@string/keyboard_code_insert_dot"
            android:keyLabel="." />

        <Key
            android:codes="@string/keyboard_code_insert_0"
            android:keyLabel="0" />

        <Key
            android:codes="@string/keyboard_code_delete"
            android:keyEdgeFlags="right" />
    </Row>

</Keyboard>