<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyHeight="30dp"
    android:keyWidth="30%p"
    android:horizontalGap="6dp"
    android:verticalGap="5dp">

    <Row>

        <Key
            android:codes="@string/keyboard_code_plus_25"
            android:keyEdgeFlags="left"
            android:keyLabel="+25" />

        <Key
            android:codes="@string/keyboard_code_plus_50"
            android:keyLabel="+50" />

        <Key
            android:codes="@string/keyboard_code_plus_75"
            android:keyLabel="+75" />

        <Key
            android:codes="@string/keyboard_code_plus_100"
            android:keyEdgeFlags="right"
            android:keyLabel="+100" />

    </Row>

    <!--1-5-->
    <Row>
        <Key
            android:codes="49"
            android:keyEdgeFlags="left"
            android:keyLabel="1" />
        <Key
            android:codes="50"
            android:keyLabel="2" />
        <Key
            android:codes="51"
            android:keyLabel="3" />
        <Key
            android:codes="52"
            android:keyLabel="4" />
        <Key
            android:codes="53"
            android:keyLabel="5" />
        <Key
            android:codes="54"
            android:keyLabel="6" />

        <Key
            android:codes="55"
            android:keyLabel="7" />
        <Key
            android:codes="56"
            android:keyLabel="8" />
        <Key
            android:codes="57"
            android:keyLabel="9" />

    </Row>

    <!--6-0-->
    <Row>

        <Key
            android:codes="@string/keyboard_code_insert_0"
            android:keyEdgeFlags="left"
            android:keyLabel="0" />
        <Key
            android:codes="@string/keyboard_code_insert_dot"
            android:keyLabel="." />
        <Key
            android:codes="@string/keyboard_code_max"
            android:keyLabel="max" />

        <Key
            android:codes="@string/keyboard_code_delete"
            android:keyLabel="" />

    </Row>



</Keyboard>