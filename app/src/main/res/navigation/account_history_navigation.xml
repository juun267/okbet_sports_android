<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/account_history_navigation"
    app:startDestination="@+id/accountHistoryNextFragment">
    <fragment
        android:id="@+id/accountHistoryFragment"
        android:name="org.cxct.sportlottery.ui.main.accountHistory.AccountHistoryFragment"
        android:label="AccountHistoryFragment">
        <action
            android:id="@+id/action_accountHistoryFragment_to_accountHistoryNextFragment"
            app:destination="@id/accountHistoryNextFragment"
            app:popUpTo="@id/accountHistoryFragment">
            <argument
                android:name="row"
                app:argType="org.cxct.sportlottery.network.bet.list.Row" />
        </action>
    </fragment>

    <fragment
        android:id="@+id/accountHistoryNextFragment"
        android:name="org.cxct.sportlottery.ui.main.accountHistory.next.AccountHistoryNextFragment"
        android:label="AccountHistoryNextFragment">
        <argument
            android:name="row"
            app:argType="org.cxct.sportlottery.network.bet.list.Row" />
    </fragment>

</navigation>