<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/identity_navigation"
    app:startDestination="@id/credentialsFragment">

    <fragment
        android:id="@+id/credentialsFragment"
        android:name="org.cxct.sportlottery.ui.profileCenter.identity.CredentialsFragment"
        android:label="fragment_credentials_new"
        tools:layout="@layout/fragment_credentials_new">
        <action
            android:id="@+id/action_credentialsFragment_to_credentialsDetailFragment"
            app:destination="@id/credentialsDetailFragment">
            <argument
                android:name="data"
                app:argType="org.cxct.sportlottery.network.credential.CredentialCompleteData"
                app:nullable="false" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/credentialsDetailFragment"
        android:name="org.cxct.sportlottery.ui.profileCenter.identity.CredentialsDetailFragment"
        android:label="CredentialsDetailFragment"
        tools:layout="@layout/fragment_credentials_detail_new">

        <argument
            android:name="data"
            app:argType="org.cxct.sportlottery.network.credential.CredentialCompleteData"
            app:nullable="false" />
    </fragment>
</navigation>