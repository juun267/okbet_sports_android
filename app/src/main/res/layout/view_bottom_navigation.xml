<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="288dp"
    android:background="@color/color_ffededed">

    <TextView
        android:id="@+id/txv_other"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="@dimen/margin_28"
        android:text="@string/other_link"
        android:textColor="@color/color_333333"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txv_about"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:text="@string/about_us"
        android:textColor="@color/color_999999"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/txv_other"
        app:layout_constraintTop_toBottomOf="@+id/txv_other" />

    <TextView
        android:id="@+id/txv_gaming"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:text="@string/responsible"
        android:textColor="@color/color_999999"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/txv_about"
        app:layout_constraintTop_toBottomOf="@+id/txv_about" />

    <TextView
        android:id="@+id/txv_terms"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:text="@string/terms_conditions"
        android:textColor="@color/color_999999"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/txv_gaming"
        app:layout_constraintTop_toBottomOf="@+id/txv_gaming" />

    <TextView
        android:id="@+id/txv_policy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:text="@string/privacy_policy"
        android:textColor="@color/color_999999"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/txv_terms"
        app:layout_constraintTop_toBottomOf="@+id/txv_terms" />

    <TextView
        android:id="@+id/txv_help"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/help"
        android:textColor="@color/color_333333"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/guideline"
        app:layout_constraintTop_toTopOf="@+id/txv_other" />

    <TextView
        android:id="@+id/txv_faq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:text="@string/faqs"
        android:textColor="@color/color_999999"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/txv_help"
        app:layout_constraintTop_toBottomOf="@+id/txv_help" />

    <TextView
        android:id="@+id/txv_chat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:text="@string/live_chat"
        android:textColor="@color/color_999999"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/txv_faq"
        app:layout_constraintTop_toBottomOf="@+id/txv_faq" />

    <TextView
        android:id="@+id/txv_contact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:text="@string/contact"
        android:textColor="@color/color_999999"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/txv_chat"
        app:layout_constraintTop_toBottomOf="@+id/txv_chat" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="14dp"
        android:src="@drawable/ic_pagcor_mark"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txv_policy" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="17dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="19dp"
        android:gravity="center_horizontal"
        android:text="@string/btm_navigation_hint"
        android:textColor="@color/color_999999"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.55" />


</androidx.constraintlayout.widget.ConstraintLayout>