<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="14dp">

    <org.cxct.sportlottery.widget.highLightTextView.HighlightTextView
        android:layout_width="0dp"
        android:id="@+id/hltv"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="25dp"
        android:gravity="center"
        android:text="@string/kyc_contact_service"
        android:textColor="@color/color_A5A9B3"
        android:textSize="@dimen/textSize14sp"
        android:maxLines="2"
        app:highlightColor="@color/transparent"
        app:highlightRadius="0dp"
        app:highlightText="@string/kyc_contact_service_hilight"
        app:highlightTextColor="@color/color_A5A9B3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>