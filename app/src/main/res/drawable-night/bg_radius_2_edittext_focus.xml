<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>

        <shape android:shape="rectangle">
            <corners android:radius="2dp" />
            <solid android:color="@color/color_232426" />
        </shape>
    </item>

    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">

        <shape android:shape="rectangle">
            <corners android:radius="2dp" />
            <stroke
                android:width="1dp"
                android:color="@color/color_6BABFF" />
            <solid android:color="@color/color_191919" />
        </shape>
    </item>
</layer-list>