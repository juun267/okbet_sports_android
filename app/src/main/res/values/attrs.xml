<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AutoScrollRecyclerView">
        <attr name="speed_dp_sec" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="CustomView">
        <attr name="cvTitle" format="string" />
        <attr name="cvCount" format="string" />
        <attr name="cvIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="ToolBarView">
        <attr name="title" format="string" />
        <attr format="reference" name="fragmentNavGraph"/>
    </declare-styleable>

    <declare-styleable name="OddsBetButton">
        <attr name="isLock" format="boolean" />
        <attr name="ouType" format="enum">
            <enum name="oType" value="0" />
            <enum name="uType" value="1" />
        </attr>
        <attr name="topText" format="string" />
        <attr name="bottomText" format="string" />
    </declare-styleable>
</resources>